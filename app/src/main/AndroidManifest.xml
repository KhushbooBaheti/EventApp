<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.eventapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".basic.StartupActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".basic.SignUpActivity" />
        <activity android:name=".basic.UserActivity" />
        <activity android:name=".basic.LogoutActivity" />
        <activity android:name=".basic.MainActivity" />
        <activity android:name=".basic.ListOfConfActivity" />
        <activity android:name=".basic.LoginActivity" >
        </activity>
    </application>

</manifest>